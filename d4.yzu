PRINT("PI:")
PRINT(MATH_PI)


LET mylist = []
PRINT("Number of elements in the list:")
LET len = INPUT_INT()
FOR i = 0 TO len STEP 1 THEN
    APPEND(mylist,INPUT())
END

PRINT("Original list:")

PRINT("List after appending:")
PRINT(mylist)
PRINT("Input the index of element that you want to pop(smaller than the len of the list):")
LET index = INPUT_INT()
PRINT(POP(mylist,index))
PRINT("List after poping:")
PRINT(mylist)
PRINT("END")
